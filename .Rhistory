library(tidyverse)
dftidy <- df %>%
pivot_longer( paste0("kpt",seq_len(20)), names_to="kpt" , values_to="id") %>%
drop_na("id")
View(dftidy)
p <- dftidy %>%
group_by(id) %>%
summarize(Count=n()) %>%
mutate(Count) %>%
arrange(desc(Count)) %>%
mutate(idx=1:nrow(.)) %>%
ggplot(aes(x=idx, y=Count)) +
geom_line() +
scale_y_log10() +
xlab("Sequentially ordered") +
ylab("Kpthesaurus Count (log-scale)") +
theme_bw()
plot(p)
